(define foo 6)
(define bar -4)

(println "foo = " foo)
(println "bar = " bar)


(println "foo + bar = " (+ foo bar))
(println "foo - bar = " (- foo bar))
(println "foo / bar = " (/ foo bar))
(println "foo * bar = " (* foo bar))
(println "foo % bar = " (% foo bar))

(define x 1.2)
(define y -.4)

(println "x = " x "\n" "y = " y)

(println "x + y = " (+ x y))
(println "x - y = " (- x y))
(println "x / y = " (/ x y))
(println "x * y = " (* x y))
(println "x % y = " (% x y))

(println "10 / (10 - (2 * (1 + 1))) = " (/ 10 (- 10 (* 2 (+ 1 1)))))
(println "x / (y - (foo * (bar + 1))) = " (/ x (- y (* foo (+ bar 1)))))